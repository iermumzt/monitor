{
  "parserOptions": {
    "ecmaVersion": 6
  },
  "env": {
    "browser": true,
    "node": true,
    "jquery": true,
    "es6": true,
    "amd": true
  },
  "rules": {
    "camelcase": 0,
    "curly": 0,
    "comma-dangle": 2,
    "brace-style": [2, "1tbs", {
      "allowSingleLine": true
    }],
    //"quotes": [2, "single"],
    "no-undef": 2,
    "no-undef-init": 2,
    "no-shadow-restricted-names": 2,
    "no-restricted-globals": 2,
    "no-label-var": 2,
    "block-scoped-var": 2,
    "array-callback-return": 2,
    "dot-location": [2, "property"],
    "no-empty-function": [2, {
      "allow": ["constructors", "methods"]
    }],
    "no-caller": 2,
    "no-alert": 2,
    "guard-for-in": 2,
    "no-with": 2,
    "wrap-iife": [2, "any"],
    "vars-on-top": 2,
    "no-unused-labels": 2,
    "no-sequences": 2,
    "no-self-compare": 2,
    "no-self-assign": 2,
    "no-return-assign": [2, "always"],
    "no-redeclare": 2,
    "no-octal-escape": 2,
    "no-native-reassign": 2,
    "no-multi-spaces": 2,
    "no-loop-func": 2,
    "no-lone-blocks": 2,
    "no-invalid-this": 2,
    "no-implied-eval": 2,
    "no-eval": 2,
    "no-empty": 2,
    "no-empty-character-class": 2,
    "no-extra-semi": 2,
    "no-inner-declarations": 2,
    "no-floating-decimal": 2,
    "no-cond-assign": 2,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-div-regex": 2,
    //"no-undefined": 2,
    "no-use-before-define": 2,
    "no-unused-vars": [2, {
      "args": "after-used"
    }],
    "semi": [2, "always"],
    "array-bracket-spacing": [2, "never"],
    "object-curly-spacing": [2, "always"],
    "space-infix-ops": 2
  },
  "globals": {
    "CURRENT_PROJECT_TIME": true,
    "ZBPlayer": true,
    "AppLog": true,
    "_hmt": true,
    "APP_CONFIG": true,
    "jQuery": true,
    "angular": true,
    "CURRENT_PROJECT_TIMESTAMP": true,
    "CURRENT_PROJECT_MODE": true,
    "CURRENT_PROJECT_DATA": true,
    "CURRENT_PROJECT_PARAMS": true,
    "BlsPlayer": true,
    "CURRENT_PROJECT_CDN_DOMAINS": true,
    "IERMU_SERVER_ROUTES": true,
    "CURRENT_PROJECT_LANGUAGE": true,
    "CURRENT_PROJECT_DIRECTIVES": true,

    "CURRENT_PROJECT_SIGN": true,
    "CURRENT_PROJECT_URL_PARAMS": true,
    "CURRENT_USERINFO": true,
    "IERMU_LANGUAGE_TRANSLATER": true,
    "CURRENT_PROJECT_EXPIRE": true,
    "CURRENT_PROJECT_TOPDOMAIN": true,
    "CURRENT_PROJECT_ACTIVITY": true,
    "ZBase": true,
    "require": true,
    "define": true,
    "IX": true,
    "AppLog": true,
    "$": true,
    "_": true,
    "console": true,
    "alert": true
  }
}