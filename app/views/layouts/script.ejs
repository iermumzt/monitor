
<% if(page_config.js !== "") {%>
  <!--[if lt IE 9]>
    <script src="http://cdn.bootcss.com/html5shiv/3.7.2/html5shiv.min.js"></script>
    <script src="http://cdn.bootcss.com/respond.js/1.4.2/respond.min.js"></script>
  <![endif]-->
  <% if(page_config.currentMode == "develop") {%>
    <%-`<script type="text/javascript" src="/public/common/require_config.js"></script>
        
        <script type="text/javascript" src="/public/js/requirejs/require.js" data-main="/public/pages/${page_config.page}/page.js"></script>`
    %>
  <% }else if(page_config.currentMode == "test") {%>
    <%-`<script type="text/javascript" src="/public/js/${page_config.commonJS}.js"></script>
      
      <script type="text/javascript" src="/public/js/${page_config.md5_js}.js"></script>`
    %>
  <%}else{%>
    <%-`<script type="text/javascript" src="${page_config.webSiteDomains[page_config.webSiteDomains.length>1?1:0]}public/js/${page_config.commonJS}.js"></script>
      
      <script type="text/javascript" src="${page_config.webSiteDomains[page_config.webSiteDomains.length>1?1:0]}public/js/${page_config.md5_js}.js"></script>`
    %>
  <%}%>
<%}%>